

inherit "/room/room";

void reset(int flag)
{
   if (flag == 0)
   {
     set_light(1);
     dest_dir = ({
     "room/forest6", "north",
     "players/padrone/outside/path2", "south",
     });
     short_desc = "On a small path",
     long_desc =
     "You are on a small path winding through the bushes.\n"+
     "To the north you can see a big dark forest.\n";
   }
}

