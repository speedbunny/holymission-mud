#lp|Generic dot-matrix printer entry:\
#        :lp=/dev/lp1:\
#        :sd=/var/spool/lp1/lp:sh:\
#        :if=/usr/bin/lpf:\
#        :df=/var/spool/lp1/filter.ps:\
#        :tf=/var/spool/lp1/filter.ps:\
#        :af=/var/spool/lp1/lp-acct:\
